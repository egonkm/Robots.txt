  <script>
    (function(){
      if (document.domain.indexOf('ch-news') === -1) {
        return;
      }
      // bind onDeviceReady
      window.onload = function() {
        document.addEventListener('deviceready', onDeviceReady, false);
      }
      // write CLSLoader script to document
      document.write('\x3Cscript src="https://scdn.line-apps.com/channel/sdk/js/LCSLoader_20150326.js">\x3C/script>');
      // apply Channel view design
      document.addEventListener("DOMContentLoaded", function(event){
        var body = document.getElementsByTagName('body')[0];
        body.classList.add('LyChannel');
        Array.prototype.forEach.call(document.getElementsByClassName('LyHead'), function(header){
          header.style.display = 'none';
        });
      });
      var onDeviceReady = function(e){
        var resVersion = '8';
        var url = location.href;
        var props = {
          entryPage: true,
          titleBarType: ['lineNewsWhite'],
          resVersion: resVersion,
          pageKey: url,
          pageURL: url,
          preventStackPop: true,
          titleBar: {
            left: {
              iconId: 'icon_back',
              visible: false,
              enable: false
            },
            center: {
              imgId: 'title',
              text: 'LINE NEWS',
              enable: true,
              clickable: true
            },
            right: {
              iconId: 'icon_close',
              visible: true,
              enable: true
            }
          }
        };
        window.LCS.Interface.updateTitleBar(props);
        window.LCS.Interface.registerTitleBarCallback(function(event){
          switch (event.target) {
            case 'BACK':
              window.LCS.Interface.closeWebView();
              break;
            case 'RBUTTON':
              window.LCS.Interface.closeWebView();
              break;
          }
        });
      }
    })();
  </script>

  <script type="text/javascript">
    _trm('send', 'pageview');
  </script>


