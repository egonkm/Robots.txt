<script>
  const cmpUtils = {};
  const cmpVendors = [];

  cmpUtils.loadVendors = function(vendors = [], checkConsent = true) {
    if (vendors && Array.isArray(vendors)) {
      const totalVendors = vendors.length;
      if (totalVendors > 0) {
        for (i = 0; i < totalVendors; i++) {
          const currentVendor = vendors[i];
          if (typeof currentVendor.load === 'function') {
            if (!checkConsent) {
              currentVendor.load(true);
            } else {
              window.__ace('djcmp', 'djcmp', ['onReady', () => {
                window.__ace('djcmp', 'customVendorIsEnabled', [currentVendor.id, currentVendor.load]);
              }]);
            }
          }
        }
      }
    }
  }

  cmpVendors.push({
    name: 'Typography',
    id: '6065f9bfeb759826af4314c7',
    load: function(isVendorEnabled = false) {
      if (isVendorEnabled) {
        console.info('CMP Custom Integrations: Loading vendor "Typography"');
        const typographyLink = document.createElement('link');
        typographyLink.setAttribute('rel', 'preload');
        typographyLink.setAttribute('as', 'style');
        typographyLink.setAttribute('type', 'text/css');
        typographyLink.setAttribute('onload', 'this.rel="stylesheet"');
        typographyLink.setAttribute('href', 'https://cloud.typography.com/7859874/7657412/css/fonts.css');
        document.body.appendChild(typographyLink);
      } else {
        console.warn('CMP Custom Integrations: Vendor "Typography" does not have consent');
      }
    }
  });

  cmpVendors.push({
    name: 'GoogleRecaptcha',
    lazyload: function (cb) {
      const googleRecaptchaScript = document.createElement('script');
      googleRecaptchaScript.setAttribute(
        'src',
        'https://www.google.com/recaptcha/api.js?onload=gCaptchaCB&render=explicit'
      );
      document.body.appendChild(googleRecaptchaScript);
    }
  });

  cmpUtils.getVendor = function (name) {
    const nxtVendor = cmpVendors.filter(function (vendor) {
      return vendor.name === name;
    });

    return nxtVendor[0];
  };

  window.cmpUtils = cmpUtils;
  window.cmpVendors = cmpVendors;

  window.__ace('djcmp', 'regulationApplies', ['cmpApiFirst', { cb: function (isAnyRegulation) {
    // check if any regulation is in place
    if (isAnyRegulation) {
      console.info('CMP Custom Integrations: regulations apply');
      cmpUtils.loadVendors(cmpVendors);
    } else {
      console.info('CMP Custom Integrations: no regulations apply');
      cmpUtils.loadVendors(cmpVendors, false);
    }
