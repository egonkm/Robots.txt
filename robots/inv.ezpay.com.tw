    <script>

        var cookie_function = new Cookie_function();
        cookie_function.set_base_url("https://inv.ezpay.com.tw/");

        $(document).ready(function () {
            $("#sm-bar").hide();
            $("#menu_btn").on("click", function () {
                $(this).toggleClass("active");
                $("#sm-bar").fadeToggle();
            });
            $("#menu_count").on("click", function (e) {
                $('html, body').animate({
                    scrollTop: $("#bg3").offset().top  // 只需修改此處
                }, 800); // 750是滑動的時間，單位為毫秒
                e.preventDefault();
            });

            $("#menu_link").on("click", function (e) {
                $('html, body').animate({
                    scrollTop: $("#bg6").offset().top  // 只需修改此處
                }, 800); // 750是滑動的時間，單位為毫秒
                e.preventDefault();
            });

            var this_location = "https://inv.ezpay.com.tw/";
            if (this_location === "https://cinv.pay2go.com/" || this_location === "https://cinv.ezpay.com.tw/") {
                $('#test_location').html('[API串接測試區]');
            } else {
                $('#test_location').html('');
            }

            var today = new Date();
            
            if(today.getFullYear() === 2020){
                if(today.getMonth() < 1){
                    $('#new_year').css('visibility','visible');
                }else if(today.getMonth() === 1 && today.getDate() <10){
