<script>
  function isMac() {
    var result = false;
    if (window.navigator.userAgent.indexOf("Mac") != -1) {
      result = true;
    }
    return result;
  }

  $ (function () {
    if (isMac()) {
      $('.is-mac').css('display','block');
      $('.btn.is-mac').css('display','inline-block');
    }

    else {
      $('.is-pc').css('display','block');
      $('.btn.is-pc').css('display','inline-block');
    }

  });
</script>
 
