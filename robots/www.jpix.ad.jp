<script src="https://www.jpix.ad.jp/assets/js/app.js"></script>

<script>
	const G_ID = "G-CEN7S7J0H1";
	const DAYS_TO_EXPIRE = 14; // Cookieの有効期限（日数）

	window.addEventListener("DOMContentLoaded", function() {
		// クエリパラメータに "clear-storage" が含まれている場合は localStorage をクリア
		if (location.search.indexOf("clear-storage") !== -1) {
			showCookieNotice(); // クリアした場合も通知を表示
		} else {
			// localStorageに保存された値が期限を超えている場合は削除
			var storedTimestamp = localStorage.getItem('timestamp');
			if (storedTimestamp) {
				var currentTimestamp = new Date().getTime();
				var timeDifference = currentTimestamp - parseInt(storedTimestamp);

				// 期限を超えていたら削除
				if (timeDifference > DAYS_TO_EXPIRE * 24 * 60 * 60 * 1000) {
					localStorage.removeItem('use_cookie');
					localStorage.removeItem('timestamp');
					showCookieNotice(); // 期限切れの場合も通知を表示
				}
			} else {
				showCookieNotice(); // localStorageにtimestampがない場合も通知を表示
			}
		}


		// gtagのオブジェクト、関数を定義
		// この時点ではトラッキングは開始されない
		window.dataLayer = window.dataLayer || [];
		window.gtag = function() {
			dataLayer.push(arguments);
		};

		// localStorage から ga_cookie_opt_in の値を取得
		var userGanalyticsOptin = localStorage.getItem('use_cookie');

		// Cookie設定の通知を表示する関数
		function showCookieNotice() {
			// 通知バーの表示
			// var acceptcss = document.createElement("link");
			// acceptcss.setAttribute("rel", "stylesheet");
			// document.head.appendChild(acceptcss);

			var accept = document.body.appendChild(document.createElement("div"));
			accept.className = "p-gdpr";
			var wrapper = accept.appendChild(document.createElement("div"));
			wrapper.className = "p-gdpr__in";
			var notice = wrapper.appendChild(document.createElement("p"));
			notice.className = "p-gdpr__text";
			notice.innerHTML = '当サイトはサービスの運営およびユーザ体験の改善を目的にCookieを使用します。詳細は、弊社の<a href="/policy" class="c-text-underline">プライバシーポリシー</a>をご参照ください。「同意する」をクリックまたは弊社サイトの利用を続行することで、弊社がCookieを使用することに同意したとみなされます。';

			var select = wrapper.appendChild(document.createElement("div"));
			var dnyBtn = select.appendChild(document.createElement("button"));
			var acpBtn = select.appendChild(document.createElement("button"));
			acpBtn.innerHTML = "同意する";
			dnyBtn.innerHTML = "拒否する";
			select.className = "p-gdpr__btn-wrap";
			acpBtn.className = "p-gdpr__accept";
			dnyBtn.className = "p-gdpr__deny";
			acpBtn.type = "button";
			dnyBtn.type = "button";

			// 通知を削除
			acpBtn.onclick = function() {
				localStorage.setItem("use_cookie", "yes");
				if (!localStorage.getItem("timestamp")) {
					localStorage.setItem("timestamp", new Date().getTime().toString());
				}
				accept.style.opacity = 0;
				accept.style.transition = "0.3s";
				setTimeout(function() {
					document.body.removeChild(accept);
					// .p-gdpr を削除
					accept.parentNode.removeChild(accept);
				}, 800);

				// トラッキング設定とgtag.jsの読み込み
				gtag("js", new Date());
				gtag("config", G_ID); // トラッキングID
				var gtagjs = document.getElementsByTagName("head")[0].appendChild(document.createElement("script"));
				gtagjs.src = "https://www.googletagmanager.com/gtag/js?id=" + G_ID; // トラッキングID

			};

			// 「同意しない」ボタンでCookie拒否
			dnyBtn.onclick = function() {
				localStorage.setItem("use_cookie", "no");
				if (!localStorage.getItem("timestamp")) {
					localStorage.setItem("timestamp", new Date().getTime().toString());
				}
				// 通知を削除
				accept.style.opacity = 0;
				accept.style.transition = "0.3s";
				setTimeout(function() {
					// document.body.removeChild(accept);
					// .p-gdpr を削除
					accept.parentNode.removeChild(accept);
				}, 800);
			};
		}

		// localStorageに保存された値に応じてCookie通知を表示
		// if (userGanalyticsOptin === "no" || userGanalyticsOptin === "yes") {
		// 	// Cookie設定が保存されている場合は何もしない
		// } else {
		// 	// Cookie設定が保存されていない場合は通知を表示
		// 	showCookieNotice();
		// }

		// Cookie設定が保存されている場合はトラッキングを開始
		if (userGanalyticsOptin === "yes") {
			gtag("js", new Date());
			gtag("config", G_ID); // トラッキングID
			var gtagjs = document.getElementsByTagName("head")[0].appendChild(document.createElement("script"));
			gtagjs.src = "https://www.googletagmanager.com/gtag/js?id=" + G_ID; // トラッキングID
		}
	});
