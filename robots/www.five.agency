    <script>
      const slider = document.getElementById('header-dropdown');
      const header = document.getElementById('header');
      const openButton = document.getElementById('openMenuBtn');
      const openButtonIcon = document.getElementById('menu-open-icon');
      const closeButtonIcon = document.getElementById('menu-close-icon');
      const dropdown = document.getElementById('header-dropdown');
      const overlay = document.getElementById('overlay');
      const isMobile = window.innerWidth < 768;
      let  lastScrollTop = 0;
      let searchText = "";


      // fix overlay height issue
      const resizeOverlay = () =>{
        const overlay = document.getElementById('overlay');
        let headerSize = header.offsetHeight;
        let newSize = document.body.offsetHeight - headerSize; // current size - header

        overlay.setAttribute("style", "height:" +newSize+"px;" );
      }

      const navigate = (url) =>{
        window.location.href = url;
      }

      addEventListener("resize", (event) => {
        let isOpen = slider.classList.contains('slide-in');

        if(isOpen){
          resizeOverlay();
        }
      }); 


      window.addEventListener("scroll", function(){
        let isOpen = slider.classList.contains('slide-in');
        let currentScroll = window.pageYOffset || document.documentElement.scrollTop;
        const mainWrapperContainer = document.getElementById('mainWrapperContainer');

        if (currentScroll > lastScrollTop) {
          // Scrolling down
          header.classList.remove('show');
          mainWrapperContainer.style.paddingTop = 0;
          scrollCount = 0; // Reset scroll count when scrolling down
        } else {
          // Scrolling up
          scrollCount++;
          header.classList.add('show');
          mainWrapperContainer.style.paddingTop = '64px';
          scrollCount = 0; // Reset scroll count after showing header
        }

        lastScrollTop = currentScroll <= 0 ? 0 : currentScroll;     

        if(document.documentElement.scrollTop < 64){
          header.classList.remove('show');
          mainWrapperContainer.style.paddingTop = 0;
        }
      });

      // Execute a function when the user presses a key on the keyboard
      const searchEnter = (event, input) =>{
        if(event.key=="Enter"){
         let searchInputText = document.getElementById(input? input: 'header-search-input').value;
          //do search
          window.location.href = "/hs-search-results?term="+searchInputText+"&type=SITE_PAGE&type=BLOG_POST&type=LISTING_PAGE";
        }
      };

      // open and close menu
      const toggleMenu = () => {
        openButtonIcon.classList.toggle("hidden");
        closeButtonIcon.classList.toggle("hidden");

        dropdown.classList.toggle('hidden');

        resizeOverlay();

        if(overlay.classList.contains('hidden')){
            overlay.classList.remove('hidden');
        }else{
          overlay.classList.add('hidden');
        }
        
        let isOpen = slider.classList.contains('slide-in');

        slider.setAttribute('class', isOpen ? 'header-dropdown hidden' : 'header-dropdown slide-in');

        slider.classList.contains('slide-in') ? document.body.style.overflow = 'hidden' : document.body.style.overflow = 'auto';
        // close open items for menu
        slider.querySelector('.block')?.classList?.remove('block');
        slider.querySelector('.animate-fade')?.classList?.remove('animate-fade');
        slider.querySelector('.rotate')?.classList?.remove('rotate');
      };

      const tabAction = (e) => {
        if (e.keyCode == 13) {
              // add your code here.
              console.log("enter");
              if(!e.srcElement.attributes.href){
                const clickEvent = new Event('click');
                e.srcElement.dispatchEvent(clickEvent);
                e.preventDefault();
              }
          }
      }

      /*
      // trigger onClick on element when pressed space for slide menu
      slider.addEventListener('keydown', function(e) {
        tabAction(e);
      });

      */

      // trigger onClick on element when pressed space for header
      header.addEventListener('keydown', function(e) {
        tabAction(e);
      });

      // load functions after dom is loaded
      document.addEventListener('DOMContentLoaded', function() {
        const accordionItems = document.querySelectorAll('.accordion-item');

        slider.setAttribute('style', 'height: calc(100vh - 80px;)');

          // close menu if clicked outside
          overlay.addEventListener('click', function () {
              const clickEvent = new Event('click');
              openButton.dispatchEvent(clickEvent);
              overlay.classList.add('hidden');
          });


          setTimeout(() => {
            slider.setAttribute('style', 'height: calc(100vh - 80px)');
          }, 50);

        // accordion functionality
        accordionItems.forEach(function(item) {
            const header = item.querySelector('.accordion-header');
            const content = item.querySelector('.accordion-content');

            header.addEventListener('click', function() {

            if(content){
              const isActive = content.classList.contains('block');

              // Close all sections
              accordionItems.forEach(function(otherItem) {
                  let hasContent =  otherItem.querySelector('.accordion-content');
                  if(hasContent){
                    otherItem.querySelector('.accordion-content').classList.remove('animate-fade');

                    setTimeout(() => {
                    content.classList.add('animate-fade');
                  }, 10);

                    otherItem.querySelector('.accordion-content').classList.remove('block');

                    if(otherItem.querySelector('.chevron')){
                      otherItem.querySelector('.chevron').classList.remove('rotate');
                    }
                  }
              });

              // Open the clicked section if it was not active, or close it if it was
              if (!isActive) {
                content.classList.add('block');
                setTimeout(() => {
                content.classList.add('animate-fade');
              }, 10);

                header.querySelector('.chevron').classList.add('rotate');
              }
            }

          });
        });
      });
    </script></div> 

