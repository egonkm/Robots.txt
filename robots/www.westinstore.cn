	<script type="text/javascript">
		$j("#cartpopup").insertAfter($j('#header .links .cart>a'));
				function formatNameMC (val) {
			var itemName = val;
			if (val && val.includes("'")){
				itemName = itemName.replace("'", " ");
			}

			return itemName;
		}
		function formatPriceMC (val, lang) {
			var price = val;
			var reg = /€|£/g;
			if (val) {
				if (lang === 'en') {
					price = price.replace(/\s|,/g, '');
				} else if (lang === 'fr') {
					price = price.replace(/,/g, '.');
					price = price.replace(/\s/g, '');
				} else if (lang === 'de' || lang === 'es') {
					price = price.replace(/\s|\./g, '');
					price = price.replace(/,/g, '.');
				}
			price = price.replace(reg, '');
			}
		
			return price;
		}
		function priceParser(price){
			return parseFloat(parseFloat(price).toFixed(2))
		}
		function updateCartView(sendAddToCartEvent = false, sendBeginCheckoutEvent = false, sendShippingEvent = false, sendPaymentEvent = false, sendCartViewEvent = false) {
			var lang = "hbeu_es";
        	lang = lang.replace('hbeu_', '');
			var brandRegex = /^Heavenly Bed - /;
			let cartViewProducts = []

			dataLayer.cartView = {
				"products": cartViewProducts,
				"totalCost": '',
				"currencyCode": 'EUR'
			};

			// Get cartProductsGA4LS in localStorage
			var cartProductsGA4LS = JSON.parse(localStorage.getItem("cartProductsGA4"));

			var cartProductsAW = []
        	var cartProductsGA4 = []

			function nameParser (name) {
				return name.replace('&amp;', '&')
					.replace(' and ', ' & ');
			}

			function handleBrandRegex(name){
				return brandRegex.test(name) ? name.split(' - ', 2).join(' - ') : name.split(' - ')[0]
			}

			// Loop through cartViewProducts, get SKUs, remove sizes from end, and push to cartProductsAW
			for (var i = 0; i < cartViewProducts.length; i++) {
				var item = cartViewProducts[i]
				var sku = cartViewProducts[i].sku
				var category = cartViewProducts[i].category
				var regex = /(-SK|-K|-Q|-D)$/g
				sku = sku.replace(regex, '')

				cartProductsAW.push({
					id: sku,
					google_business_vertical: 'retail'
				})
				
				var fullProdName = nameParser(cartViewProducts[i].name);
				var productName;
				var itemVariants;

				// Remove product size from end of name
				productName = handleBrandRegex(fullProdName);

				itemVariants = fullProdName.replaceAll(productName, '')
				itemVariants = itemVariants
								.replace('The ', '')
								.replace(' - ', '')
								.replaceAll(' - ', ';')
								.replaceAll(' x ', 'x')
								.replaceAll(' cm', 'cm');

				itemData = {
					'item_name': productName,
					'price': parseFloat(item.price),
					'item_category': category,
					'item_list_name': category,
					'item_list_id': category,
					'quantity': parseFloat(item.quantity),
					'discount': priceParser(item.discount),
					'subscription': 'no'
				}

				// Add discount data
				if (item.discount > 0) {
				}

				// Add item variant 
				if (itemVariants != '') {
					itemData['item_variant'] = itemVariants;
				}

				if (cartProductsGA4LS != null) {
					cartProductsGA4LS.forEach(gaItem => {
						if (productName === gaItem.item_name){
							itemData['item_id'] = gaItem.item_id;
						}
					});
				}

				cartProductsGA4.push(itemData) 
			}
			if (sendBeginCheckoutEvent) {
				// Loop through cartProductsGA4, add new properties and push to cartProductsAW
				for (var i = 0; i < cartProductsGA4.length; i++) {
					cartProductsGA4[i]['affiliation'] = 'Westin';
					cartProductsGA4[i]['index'] = i;
					cartProductsGA4[i]['item_brand'] = 'Westin Store';
				}
				window.dataLayer = window.dataLayer || [];
				dataLayer.push({ ecommerce: null });
				dataLayer.push({
					event: 'begin_checkout',
					ecommerce: {
						value: 0,
						currency: 'EUR',
						language: lang,
						items: cartProductsGA4
					}
				})
			}
			// trigger add_shipping_info event
			if (sendShippingEvent) {
				// Loop through cartProductsGA4, add new properties and push to cartProductsAW
				for (var i = 0; i < cartProductsGA4.length; i++) {
					cartProductsGA4[i]['affiliation'] = 'Westin';
					cartProductsGA4[i]['index'] = i;
					cartProductsGA4[i]['item_brand'] = 'Westin Store';
				}

				window.dataLayer = window.dataLayer || [];
				dataLayer.push({ ecommerce: null }); // Clear the previous ecommerce object.
				dataLayer.push({
					event: 'add_shipping_info',
					ecommerce: {
						currency: 'EUR',
						value: 0,
						shipping_tier: 'Standard',
						language: lang,
						items: cartProductsGA4
					}
				})
			}
			// trigger add_payment_info event
			if (sendPaymentEvent) {
				try {
					var creditCard = document.querySelector("#p_method_ops_cc");
					var bankTransfer = document.querySelector("#p_method_banktransfer");
					var cheque = document.querySelector("#p_method_checkmo");
					var paymentType = '';

					if (creditCard.checked) {
						paymentType = 'Credit Card'
					} else if (bankTransfer.checked) {
						paymentType = 'Bank Transfer'
					} else if (cheque.checked) {
						paymentType = 'Cheque'
					}
				} catch (e) {
					console.log(e);
				}

